{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "projects": {
    "discord": {
      "type": "library",
      "root": "libs/discord",
      "entryFile": "index",
      "sourceRoot": "libs/discord/src",
      "compilerOptions": {
        "tsConfigPath": "libs/discord/tsconfig.lib.json"
      }
    },
    "engine": {
      "type": "library",
      "root": "libs/engine",
      "entryFile": "index",
      "sourceRoot": "libs/engine/src",
      "compilerOptions": {
        "tsConfigPath": "libs/engine/tsconfig.lib.json"
      }
    },
    "block": {
      "type": "library",
      "root": "libs/block",
      "entryFile": "index",
      "sourceRoot": "libs/block/src",
      "compilerOptions": {
        "tsConfigPath": "libs/block/tsconfig.lib.json"
      }
    }
  },
  "compilerOptions": {
    "webpack": true
  }
}